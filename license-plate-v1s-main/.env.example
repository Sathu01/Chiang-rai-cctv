# Copy this file to `.env.dev` for development or `.env.prod` for production and adjust values

# Environment selection: dev or prod
ENVIRONMENT=dev

# Kafka settings
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC=license-plates
KAFKA_GROUP_ID=lp-consumer-group
KAFKA_AUTO_OFFSET_RESET=earliest

# Model path
MODEL_PATH=models/license-plate-finetune-v1s.pt
CONFIDENCE_THRESHOLD=0.25

# OCR Engine Selection
OCR_ENGINE=gemini
# Gemini API Settings
GEMINI_API_KEY=your_gemini_api_key_here
#Gemini Model Options
GEMINI_MODEL=gemini-3-pro-image-preview

# Gemini Advanced Settings
GEMINI_TEMPERATURE=0.1
GEMINI_MAX_RETRIES=3
GEMINI_TIMEOUT=30

#  Output Settings 
OUTPUT_IMAGE_DIR=output/images
OUTPUT_JSON_DIR=output/json